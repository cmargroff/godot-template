{
  "$schema": "http://json.schemastore.org/template",
  "author": "Chris Margroff",
  "classifications": [
    "Game"
  ],
  "identity": "Godot.Template",
  "name": "Godot project",
  "shortName": "godot",
  "sourceName": "ProjectNamePlaceholder",
  "tags": {
    "language": "C#",
    "type": "solution"
  },
  "sources": [
    {
      "source": "./",
      "target": "./",
      "include": "**/*",
      "exclude": [
        ".git/**",
        ".godot/**",
        ".template.config/**",
        "README.md",
        "**/*.import"
      ]
    }
  ],
  "symbols": {
    "projectName": {
      "type": "parameter",
      "isRequired": true,
      "datatype": "string",
      "replaces": "ProjectNamePlaceholder"
    }
  },
  "modifiers": [
    {
      "condition": "true",
      "include": "**/*",
      "exclude": []
    }
  ]
}